{
  "compilerOptions": {
    "target": "es2020",
    "baseUrl": "./",
    "moduleResolution": "node",
    "checkJs": true,
    "allowJs": true,
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "paths": {
      "~/*": ["./*"],
      "page-objects/*": ["./test/page-objects/*"],
      "components/*": ["./test/components/*"]
    }
  },
  "exclude": ["node_modules", "build", "docker"]
}
